{
  "app": {
    "background": {
      "scripts": [
        "background/background.js"
      ]
    }
  },
  "manifest_version": 2,
  "name": "Instant Music",
  "version": "0.301",
  "default_locale": "en",
  "description": "Play music without effort",
  "icons": {
    "16": "assets/icons/iconx16.png",
    "32": "assets/icons/iconx32.png",
    "48": "assets/icons/iconx48.png",
    "128": "assets/icons/iconx128.png"
  },
  "author": "Howon",
  "file_system_provider_capabilities": {
    "configurable": true,
    "multiple_mounts": true,
    "source": "network"
  },
  "offline_enabled": false,
  "permissions": ["webview", "browser", "http://www.youtube.com/*",
    "https://www.youtube.com/*", "https://fonts.googleapis.com/*",
    "https://www.google-analytics.com/", "https://ssl.google-analytics.com/",
    "storage", "notifications", "identity", "background"
  ],
  "oauth2": {
    // PRODUCTION: 
    "client_id": "962679610062-g98hk06dc820d1egmabhcht7a7l8v1tq.apps.googleusercontent.com",
    // DEVELOPMENT: kpfmenmbohaoklohdhplnchfkngfhdhl
    // "client_id": "962679610062-hs01ssioiq81lgqa34us84ufbn57ur6h.apps.googleusercontent.com",
    "scopes": [
      "email"
    ]
  },
  "sandbox": {
    "pages": ["sandbox/sandbox.html"]
  },
  "webview": {
    "partitions": [{
      "name": "static",
      "accessible_resources": [
        "sandbox/sandbox.html",
        "sandbox/sandbox.js",
        "sandbox/sandbox.css",
        "sandbox/gatrack.js",
        "sandbox/jquery-1.12.2.js"
      ]
    }]
  },
  "short_name": "Instant Music App",
  "version_name": "Beta",
  "commands": {
    "TOGGLE_PLAY_PAUSE": {
      "suggested_key": {
        "default": "Ctrl+Shift+Down"
      },
      "description": "Toggle Play/Pause"
    },
    "PLAY_NEXT_SONG": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right"
      },
      "description": "Play Next Song"
    },
    "PLAY_PREV_SONG": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left"
      },
      "description": "Play Previous Song"
    }
  }
}
